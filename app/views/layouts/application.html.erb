<!DOCTYPE html>
<html>
<head>
  <title>SoulivongBlog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<!-- navbar -->
  <%= render "/partials/header" %>

<% flash.each do |key, value| %>
<p><%= value %></p>
<% end %>

<div class="container">
  <div class="row">
    <div class="col-sm-8">
      <%= yield %>
    </div>
  </div>
</div>
</body>
</html>
